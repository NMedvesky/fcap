boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/andy.png"
boot_splash/fullsize=false
config/icon="res://assets/andy.png"

[autoload]

Global="*res://scripts/Global.gd"
BaseStudent="*res://scripts/BaseStudent.gd"
Students="*res://scripts/Students.gd"
SignalBus="*res://scripts/SignalBus.gd"
SceneSwitcher="*res://scripts/scene_switcher.gd"

[display]

window/stretch/mode="canvas_items"

[gui]

theme/custom_font="res://assets/Consolas.ttf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.046926, 0.0661713, 0.09315, 1)
